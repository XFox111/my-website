{
	"extends": [
		"next/core-web-vitals",
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"react/no-unescaped-entities": "off",
		"indent": [
			"warn",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		],
		"no-unused-vars": "warn",
		"react/react-in-jsx-scope": "off",
		"react/jsx-uses-react": "off",
		"no-unreachable": "warn"
	}
}
